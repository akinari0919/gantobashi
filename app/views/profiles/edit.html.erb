<main>
  <div class = 'top-contents'>
    <%= form_with model: @user, url: profile_path, local: true do |f| %>
      <%= f.label :name, 'Name' %>
      <br>
      <%= f.text_field :name %>
      <br>
      <%= f.label :email, 'Email' %>
      <br>
      <%= f.text_field :email  %>
      <br>
      <%= f.submit 'Update' %>
    <% end %>
    <%= link_to 'Back', profile_path %>
  </div>
</main>
